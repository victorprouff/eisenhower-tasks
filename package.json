{
  "name": "eisenhower-tasks",
  "version": "1.6.0",
  "description": "Application de gestion des t√¢ches avec la matrice d'Eisenhower",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build:mac": "electron-builder --mac",
    "build:win": "electron-builder --win",
    "build:linux": "electron-builder --linux"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victorprouff/eisenhower-tasks.git"
  },
  "keywords": [
    "eisenhower",
    "productivity",
    "task-management"
  ],
  "author": "Victor Prouff",
  "license": "MIT",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/victorprouff/eisenhower-tasks/issues"
  },
  "homepage": "https://github.com/victorprouff/eisenhower-tasks#readme",
  "build": {
    "appId": "com.eisenhower.tasks",
    "productName": "Eisenhower Tasks",
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "dmg",
        "zip"
      ],
    "icon": "build/icon.icns"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "build/icon.png"
    },
    "files": [
      "main.js",
      "preload.js",
      "renderer.js",
      "index.html",
      "styles.css",
      "build/icon.*"
    ]
  },
  "devDependencies": {
    "electron": "^40.2.1",
    "electron-builder": "^26.7.0"
  }
}